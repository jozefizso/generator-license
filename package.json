{
  "name": "generator-license",
  "version": "5.5.0",
  "description": "License generator for Yeoman based projects.",
  "homepage": "https://github.com/jozefizso/generator-license",
  "author": {
    "name": "Jozef Izso",
    "url": "https://github.com/jozefizso"
  },
  "files": [
    "app"
  ],
  "main": "app/index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "license"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.0",
    "jest": "^25.1.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.7.0"
  },
  "bugs": "https://github.com/jozefizso/generator-license/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jozefizso/generator-license.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "jest && eslint **/*.js",
    "prepublishOnly": "npm audit --audit-level=high",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "yeoman-generator": "^4.13.0"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
